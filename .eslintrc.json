  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "import/prefer-default-export": 0,
    "arrow-body-style": 0
  },
  "plugins": ["prettier", "react"],
  "extends": ["airbnb-base", "prettier", "plugin:react/recommended"]
}
